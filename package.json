			"import": "./dist/aegis-event-tracking.es.js",
			"require": "./dist/aegis-event-tracking.umd.js"
		}
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"vue": "^3.3.4",
		"vue-router": "^4.2.2"
	},
	"devDependencies": {
		"@types/node": "^20.2.5",
		"@vitejs/plugin-vue": "^4.1.0",
		"typescript": "^5.0.2",
		"vite": "^4.3.2",
		"vite-plugin-dts": "^2.3.0",
		"vue-tsc": "^1.4.2"
	}
}
